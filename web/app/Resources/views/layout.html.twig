        <title>{% block title %}Центр крови{% endblock %}</title>
        {% block stylesheets %}

            {% stylesheets 'bundles/bloodcentermain/css/*' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
